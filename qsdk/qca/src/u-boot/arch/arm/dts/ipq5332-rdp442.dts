#include "ipq5332.dtsi"

/ {
	config_name = "config@mi01.3", "config@rdp442", "config-rdp442";

	aliases {
		console = &blsp1_uart1;
		sdhci = &mmc;
	};

	soc: soc {
		tlmm: pinctrl@1000000 {
			sdhci_pinmux: mmc {
				emmc_data {
					pins = "GPIO_8", "GPIO_9", "GPIO_10",
						"GPIO_11";
					function = "sdc";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-pull-up;
				};

				emmc_cmd{
					pins = "GPIO_12";
					function = "sdc";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-pull-up;
				};

				emmc_clk{
					pins = "GPIO_13";
					function = "sdc";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-disable;
				};
			};

			qspi_pinmux: nand {
				qspi_data {
					pins = "GPIO_11", "GPIO_10", "GPIO_9",
						"GPIO_8";
					function = "qspi";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-pull-down;
				};

				qspi_cs_n {
					pins = "GPIO_12";
					function = "qspi";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-pull-up;
				};

				qspi_clk {
					pins = "GPIO_13";
					function = "qspi";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-pull-down;
				};
			};

			serial_pinmux: uart {
				data {
					pins = "GPIO_18", "GPIO_19";
					function = "blsp0_uart0";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-pull-up;
				};
			};

			blsp1_spi_pinmux: blsp1_spi_pins {
				blsp0_spi_clk {
					pins = "GPIO_14";
					function = "blsp0_spi";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-disable;
				};

				blsp0_spi_mosi {
					pins = "GPIO_15";
					function = "blsp0_spi";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-disable;
				};

				blsp0_spi_miso {
					pins = "GPIO_16";
					function = "blsp0_spi";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-disable;
				};

				blsp0_spi_cs {
					pins = "GPIO_17";
					function = "blsp0_spi";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-pull-up;
				};

			};

			mdio_pinmux: mdio {
				mdc {
					pins = "GPIO_27";
					function = "mdc_1";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-disable;
				};

				mdio {
					pins = "GPIO_28";
					function = "mdio_1";
					drive-strength = <DRIVE_STRENGTH_8MA>;
					bias-pull-up;
				};
			};

			phy_rst_pinmux: phy_rst {
				qti_8x8x_rst {
					pins = "GPIO_51";
					bias-pull-up;
				};
			};
		};

		nand: nand-controller@79B0000 {
			pinctrl-names = "default";
			pinctrl-0 = <&qspi_pinmux>;
			status = "okay";
		};

		blsp1_uart1: serial@78af000 {
			pinctrl-names = "uart";
			pinctrl-0 = <&serial_pinmux>;
			status = "okay";
		};

		blsp1_spi: spi@78b5000 {
			pinctrl-names = "default";
			pinctrl-0 = <&blsp1_spi_pinmux>;
			cs-gpios = <&tlmm 17 0>;
			force-cs;
			status = "okay";

			gd25lq32@0 {
				compatible = "jedec,spi-nor";
				reg = <0>;
			};
		};

		mmc: sdhci@7800000 {
			pinctrl-names = "mmc";
			pinctrl-0 = <&sdhci_pinmux>;
			status = "okay";
		};

		mdio: mdio@90000 {
			pinctrl-names = "mdio";
			pinctrl-0 = <&mdio_pinmux>;
			status = "okay";

			phy0: ethernet-switch@1 {
				phy-reset-gpio = <&tlmm 51 0>;
				dev-mode = <0x1>;
				reg = <1>;
				phy_id = <10>;
				id = <1>;
				uniphy_id = <0>;
				uniphy_type = <0>;
				max_speed = <2500>;
				force-speed;
				xgmac;
				ports {
					#address-cells = <1>;
					#size-cells = <0>;

					port0: port@0 {
						reg = <0>;
						label = "cpu";
						phy-mode = "sgmii-2500";
						fixed-link {
							speed = <2500>;
							full-duplex;
						};
					};

					port1: port@1 {
						reg = <1>;
						label = "lan";
					};

					port2: port@2 {
						reg = <2>;
						label = "lan";
					};

					port3: port@3 {
						reg = <3>;
						label = "lan";
					};

					port4: port@4 {
						reg = <4>;
						label = "lan";
					};
				};
			};

			phy1: phy@ff {
				reg = <0xFF>;
				phy_id = <5>;
				id = <2>;
				uniphy_id = <1>;
				uniphy_type = <0>;
				max_speed = <10000>;
				xgmac;
			};
		};

		ethernet: nss-switch {
			phy-handle0 = <&phy0>;
			phy-handle1 = <&phy1>;
			status = "okay";
		};
	};
};
